super + n
	kill -44 $(pidof nmtui) & urxvt -tn rxvt-unicode -name nmtui -e nmtui

super + shift + n
	arandr

super + w
	$BROWSER

super + shift + w
	$PWBROWSER

super + e
	thunderbird

super + v
	vscode

super + f
	spacefm

super + Print
	cd ~/pix/scrot && scrot

super + shift + Print
	cd ~/pix/scrot && scrot -s

XF86AudioRaiseVolume
	pamixer -u; pamixer --allow-boost -i 5

XF86AudioLowerVolume
	pamixer -u; pamixer --allow-boost -d 5

XF86AudioMute
	pamixer -t

XF86Sleep
	lock.sh

XF86ScreenSaver	
	lock.sh 

XF86Display
	desktop_mode.sh

shift + XF86Display
	autorandr --change

XF86TouchpadToggle
	touchpad.sh

super + s
	tdrop -w "" -h "" -x "" -y "" urxvt -name spterm

super + r
	 tdrop -w "980" -h "650" -x "" -y "" keepassxc

super + shift + r
	 tdrop -w "980" -h "650" -x "" -y "" bitwarden

super + m
	tdrop -w "800" -h "500" -x "" -y "" plexamp
